<%= form_for [@artist, @song], :html => {:multipart => true} do |f| %>
	<p>
    <%= f.label :lyric, :class => 'control-label' %>
    
    <%= f.text_area :lyric, :class => 'text_area' %>
    </p>
    <p>
    <%= f.label :name, :class => 'control-label' %>
   
    <%= f.text_field :name, :class => 'text_field' %>
	</p>
	<p>
		<%= f.file_field :image %>
	</p>
	<p> 
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")), artist_songs_path, :class => 'btn' %>
	</p>  
<% end %>

